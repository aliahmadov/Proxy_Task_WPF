<Window x:Class="ProxyTask_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProxyTask_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label FontSize="25" Margin="10">TextBlock</Label>
            <TextBlock Name="textBlock" Margin="10" FontSize="20" TextWrapping="Wrap"></TextBlock>
            <StackPanel Orientation="Horizontal">
                <TextBox TextChanged="textBox_TextChanged" Name="textBox" FontSize="25" Margin="10" Width="600"></TextBox>
                <Button Click="Button_Click" FontSize="35" Margin="10" Width="150">Submit</Button>
            </StackPanel>

            <ListBox Name="listBox">
                
            </ListBox>
        </StackPanel>
    </Grid>
</Window>
